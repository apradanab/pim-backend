{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=prod node dist/index.js",
    "start:dev": "cross-env DEBUG=PIM* NODE_ENV=dev PORT=3400 node --watch --env-file=.env dist/index.js",
    "start:mon": "cross-env DEBUG=PIM* NODE_ENV=dev PORT=3400 nodemon dist/index.js",
    "build": "tsc",
    "dev": "tsc -w",
    "test": "cross-env NODE_ENV=test jest",
    "test:dev": "cross-env NODE_ENV=test jest --watchAll --coverage",
    "test:prod": "jest --watchAll --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "Alejandro",
  "prettier": {
    "singleQuote": true
  },
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  }
